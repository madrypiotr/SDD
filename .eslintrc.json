{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "array-bracket-spacing": ["error", "never"],
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": [2, {"after": true, "before": true}],
    "brace-style": [2, "1tbs"],
    "comma-dangle": [2, "never"],
    "comma-style": [2, "last"],
    "eol-last": 2,
    "func-call-spacing": [2, "never"],
    "indent": [2, 4],
    "keyword-spacing": [2, {"after": true, "before": true}],
    "linebreak-style": [2, "unix"],
    "no-else-return": 2,
    "no-trailing-spaces": 2,
    "object-curly-spacing": [2, "never"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}]
  },
  "globals": {
    "$": false,
    "Accounts": true,
    "App": true,
    "Assets": true,
    "Curl": true,
    "Email": true,
    "EmailError": true,
    "Errors": true,
    "KWESTIA_ACTION": true,
    "KWESTIA_STATUS": true,
    "KWESTIA_TYPE": true,
    "Kwestia": true,
    "Languages": true,
    "Log": false,
    "Message": true,
    "Meteor": false,
    "Mongo": false,
    "NOTIFICATION_TYPE": true,
    "Parametr": true,
    "ParametrDraft": true,
    "Posts": true,
    "Powiadomienie": true,
    "Raport": true,
    "Rodzaj": true,
    "Roles": false,
    "Router": false,
    "SSR": true,
    "Subroles": true,
    "TAPi18n": false,
    "Temat": true,
    "Template": false,
    "USERTYPE": true,
    "Users": true,
    "UsersDraft": true,
    "ZespolRealizacyjny": true,
    "_": false,
    "moment": false,
    "throwError": false
  }
}
